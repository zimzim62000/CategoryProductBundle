{% extends 'ZIMZIMCategoryProductBundle::layout.html.twig' %}


{% block title %}
    {{ 'zimzim_categoryproduct_product.index.title'|trans }}
{% endblock title %}


{% block page %}
    <div class="row">
        <div class="small-12 columns">
            <h1 class="text-center headerpagehome">
                Entity
            </h1>
        </div>
    </div>
    <div class="row">
        <div class="small-12 columns">
            <table class="width-100 panel">
                <tbody>
                    <tr>
                        <th>Id</th>
                            <td>{{ entity.id }}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                            <td>{{ entity.name }}</td>
                    </tr>
                    <tr>
                        <th>Title</th>
                            <td>{{ entity.title }}</td>
                    </tr>
                    <tr>
                        <th>Description</th>
                            <td>{{ entity.description }}</td>
                    </tr>
                    <tr>
                        <th>Feature</th>
                            <td>{{ entity.feature }}</td>
                    </tr>
                    <tr>
                        <th>Listing</th>
                            <td>{{ entity.listing }}</td>
                    </tr>
                    <tr>
                        <th>Specification</th>
                            <td>{{ entity.specification }}</td>
                    </tr>
                    <tr>
                        <th>Createdat</th>
                            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
                    </tr>
                    <tr>
                        <th>Updatedat</th>
                            <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
                    </tr>
                    <tr>
                        <th>Path1</th>
                            <td>{{ entity.path1 }}</td>
                    </tr>
                    <tr>
                        <th>Path2</th>
                            <td>{{ entity.path2 }}</td>
                    </tr>
                    <tr>
                        <th>Path3</th>
                            <td>{{ entity.path3 }}</td>
                    </tr>
                    <tr>
                        <th>Path4</th>
                            <td>{{ entity.path4 }}</td>
                    </tr>
                </tbody>
            </table>

                        <div class="row">
    <div class="small-12 large-4 columns text-center">
        <a class="tiny button round" href="{{ path('zimzim_categoryproduct_product') }}">
            {{ 'link.return'|trans }}
        </a>
    </div>
    <div class="small-12 large-4 columns text-center">
        <a class="tiny button round" href="{{ path('zimzim_categoryproduct_product_edit', { 'id': entity.id }) }}">
            {{ 'button.update'|trans }}
        </a>
    </div>
    <div class="small-12 large-4 columns text-center">
        {{ form(delete_form) }}
    </div>
</div>
        </div>
    </div>
{% endblock %}

